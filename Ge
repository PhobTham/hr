local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Pro Stealer", "DarkTheme")

local Tab = Window:NewTab("Buy Hand")


local Section = Tab:NewSection("buy hand")

Section:NewButton("Hand rainbow (1,000,000$)", "1M", function() local args = { [1] = "Hand Rainbow" } game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end)

